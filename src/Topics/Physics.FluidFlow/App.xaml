<local:PhysicsApp
    x:Class="Physics.FluidFlow.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Physics.FluidFlow"
    xmlns:selfstudy="using:Physics.SelfStudy.Resources" xmlns:converters="using:Physics.FluidFlow.Converters"
	RequestedTheme="Light">

    <local:PhysicsApp.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <XamlControlsResources xmlns="using:Microsoft.UI.Xaml.Controls" />
                <ResourceDictionary Source="ms-appx:///Physics.Shared.UI/Resources/DefaultResources.xaml" />
                <selfstudy:SelfStudyResources />
            </ResourceDictionary.MergedDictionaries>
            <Color x:Key="AppThemeColor">#166025</Color>
            <Color x:Key="DarkAppThemeColor">#0C3815</Color>
            <Color x:Key="AppTitleBarColor">#0C3815</Color>
		</ResourceDictionary>
    </local:PhysicsApp.Resources>

</local:PhysicsApp>
