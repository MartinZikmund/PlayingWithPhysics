<local:PhysicsApp
    x:Class="Physics.DragMovement.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Physics.DragMovement"
    xmlns:resources="using:Physics.SelfStudy.Resources"
    RequestedTheme="Light">
    <local:PhysicsApp.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <XamlControlsResources xmlns="using:Microsoft.UI.Xaml.Controls" />
                <ResourceDictionary Source="ms-appx:///Physics.Shared.UI/Resources/DefaultResources.xaml" />
                <resources:SelfStudyResources />
            </ResourceDictionary.MergedDictionaries>
            <Color x:Key="AppThemeColor">#6b97ad</Color>
			<Color x:Key="AppTitleBarColor">#0D2B4A</Color>
			<Color x:Key="DarkAppThemeColor">#124E87</Color>
        </ResourceDictionary>
    </local:PhysicsApp.Resources>
</local:PhysicsApp>
