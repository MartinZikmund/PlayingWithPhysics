<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Physics.HomogenousMovement.Assets.Resources"
    xmlns:toolkitPrimitives="using:Microsoft.Toolkit.Uwp.UI.Controls.Primitives">

    <Style x:Key="ValuesTableCloseButtonStyle" TargetType="Button">
        <Setter Property="Background" Value="#ff4299dd"></Setter>
    </Style>

    <Style x:Key="ValuesTableColumnHeaderStyle" TargetType="toolkitPrimitives:DataGridColumnHeader">
        <Setter Property="FontWeight" Value="Bold" />
        <Setter Property="Foreground" Value="#ff4299dd" />
    </Style>
</ResourceDictionary>
