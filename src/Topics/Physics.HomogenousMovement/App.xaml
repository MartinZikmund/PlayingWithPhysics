
<local:PhysicsApp
    x:Class="Physics.HomogenousMovement.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Physics.HomogenousMovement"
    RequestedTheme="Light">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <XamlControlsResources xmlns="using:Microsoft.UI.Xaml.Controls" />
                <ResourceDictionary Source="ms-appx:///Physics.Shared/Resources/DefaultResources.xaml" />
                <ResourceDictionary Source="/Assets/Resources/ValuesTableResources.xaml" />
            </ResourceDictionary.MergedDictionaries>
            <Color x:Key="AppThemeColor">#0072b0</Color>
            <Color x:Key="DarkAppThemeColor">#0072b0</Color>
        </ResourceDictionary>
    </Application.Resources>
</local:PhysicsApp>
