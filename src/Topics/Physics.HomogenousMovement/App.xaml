
<local:HomogenousMovementApp
    x:Class="Physics.HomogenousMovement.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Physics.HomogenousMovement"
    xmlns:resources="using:Physics.SelfStudy.Resources"
    RequestedTheme="Light">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <XamlControlsResources xmlns="using:Microsoft.UI.Xaml.Controls" />
                <ResourceDictionary Source="ms-appx:///Physics.Shared.UI/Resources/DefaultResources.xaml" />
                <resources:SelfStudyResources />
            </ResourceDictionary.MergedDictionaries>
            <Color x:Key="AppThemeColor">#0072b0</Color>
            <Color x:Key="AppTitleBarColor">#0D2B4A</Color>
            <Color x:Key="DarkAppThemeColor">#0072b0</Color>
            <ElementTheme x:Key="AppContrastTheme">Dark</ElementTheme>
        </ResourceDictionary>
    </Application.Resources>
</local:HomogenousMovementApp>
