<local:PhysicsApp
    x:Class="Physics.WaveInterference.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Physics.WaveInterference"
    xmlns:resources="using:Physics.SelfStudy.Resources"
    RequestedTheme="Light">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <XamlControlsResources xmlns="using:Microsoft.UI.Xaml.Controls" />
                <ResourceDictionary Source="ms-appx:///Physics.Shared.UI/Resources/DefaultResources.xaml" />
                <resources:SelfStudyResources />
            </ResourceDictionary.MergedDictionaries>
            <Color x:Key="AppThemeColor">#edb901</Color>
            <Color x:Key="AppTitleBarColor">#edb901</Color>
            <Color x:Key="DarkAppThemeColor">#155f8f</Color>
        </ResourceDictionary>
    </Application.Resources>
</local:PhysicsApp>
