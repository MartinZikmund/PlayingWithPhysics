<local:PhysicsApp
    x:Class="Physics.LawOfConservationOfMomentum.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Physics.LawOfConservationOfMomentum"
    xmlns:selfstudy="using:Physics.SelfStudy.Resources"
    RequestedTheme="Light">

    <local:PhysicsApp.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <XamlControlsResources xmlns="using:Microsoft.UI.Xaml.Controls" />
                <ResourceDictionary Source="ms-appx:///Physics.Shared.UI/Resources/DefaultResources.xaml" />
                <selfstudy:SelfStudyResources />
            </ResourceDictionary.MergedDictionaries>
            <Color x:Key="AppThemeColor">#A5D3D3</Color>
            <Color x:Key="DarkAppThemeColor">#83a8a8</Color>
            <Color x:Key="AppTitleBarColor">#A5D3D3</Color>
        </ResourceDictionary>
    </local:PhysicsApp.Resources>

</local:PhysicsApp>
