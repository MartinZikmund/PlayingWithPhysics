<local:PhysicsApp
    x:Class="Physics.HomogenousParticle.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Physics.HomogenousParticle"
    xmlns:resources="using:Physics.HomogenousParticle.Resources"
    RequestedTheme="Light">
    <local:PhysicsApp.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <XamlControlsResources xmlns="using:Microsoft.UI.Xaml.Controls" />
                <ResourceDictionary Source="ms-appx:///Physics.Shared.UI/Resources/DefaultResources.xaml" />
                <resources:DataTemplates />
            </ResourceDictionary.MergedDictionaries>
            <Color x:Key="AppThemeColor">#4fd5fa</Color>
            <Color x:Key="DarkAppThemeColor">#124E87</Color>
        </ResourceDictionary>
    </local:PhysicsApp.Resources>
</local:PhysicsApp>
