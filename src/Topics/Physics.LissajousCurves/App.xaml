<local:PhysicsApp
    x:Class="Physics.LissajousCurves.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Physics.LissajousCurves"
    xmlns:resources="using:Physics.SelfStudy.Resources"
    RequestedTheme="Light">
	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<XamlControlsResources xmlns="using:Microsoft.UI.Xaml.Controls" />
				<ResourceDictionary Source="ms-appx:///Physics.Shared.UI/Resources/DefaultResources.xaml" />
				<resources:SelfStudyResources />
			</ResourceDictionary.MergedDictionaries>
			<Color x:Key="AppThemeColor">#287aaf</Color>
			<Color x:Key="AppTitleBarColor">#1a6ca1</Color>
			<Color x:Key="DarkAppThemeColor">#155f8f</Color>
		</ResourceDictionary>
	</Application.Resources>
</local:PhysicsApp>
