<local:PhysicsApp
    x:Class="Physics.ElectricParticle.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Physics.ElectricParticle"
    xmlns:resources="using:Physics.ElectricParticle.Resources"
    xmlns:resources1="using:Physics.SelfStudy.Resources"
    RequestedTheme="Light">
    <local:PhysicsApp.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <XamlControlsResources xmlns="using:Microsoft.UI.Xaml.Controls" />
                <ResourceDictionary Source="ms-appx:///Physics.Shared.UI/Resources/DefaultResources.xaml" />
                <resources:DataTemplates />
                <resources1:SelfStudyResources />
                <ResourceDictionary Source="ms-appx:///Resources/GameResources.xaml" />
            </ResourceDictionary.MergedDictionaries>
            <x:Double x:Key="ContentDialogMaxWidth">800</x:Double>
            <Color x:Key="AppThemeColor">#FD5800</Color>
            <Color x:Key="DarkAppThemeColor">#124E87</Color>
            <Color x:Key="AppTitleBarColor">#FD5800</Color>
        </ResourceDictionary>
    </local:PhysicsApp.Resources>
</local:PhysicsApp>
