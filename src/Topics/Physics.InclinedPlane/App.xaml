<local:PhysicsApp
    x:Class="Physics.InclinedPlane.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Physics.InclinedPlane"
    xmlns:resources="using:Physics.SelfStudy.Resources"
    RequestedTheme="Light">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <XamlControlsResources xmlns="using:Microsoft.UI.Xaml.Controls" />
                <ResourceDictionary Source="ms-appx:///Physics.Shared.UI/Resources/DefaultResources.xaml" />
                <ResourceDictionary Source="/Resources/DataTemplates.xaml" />
                <resources:SelfStudyResources />
            </ResourceDictionary.MergedDictionaries>
            <Color x:Key="AppThemeColor">#dd6285</Color>
            <Color x:Key="AppTitleBarColor">#9e3f5a</Color>
            <Color x:Key="DarkAppThemeColor">#ad4e69</Color>
        </ResourceDictionary>
    </Application.Resources>
</local:PhysicsApp>
