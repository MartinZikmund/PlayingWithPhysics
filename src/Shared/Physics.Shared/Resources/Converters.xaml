<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:converters="using:Physics.Shared.UI.Converters"
    xmlns:local="using:Physics.Shared.Resources">
    <converters:NullToVisibilityConverter x:Key="NullToVisibilityConverter" />
    <converters:NullToVisibilityConverter x:Key="InvertedNullToVisibilityConverter" Invert="True" />
    <converters:NativeBoolToVisibilityConverter x:Key="BoolToVisibilityConverter" />
    <converters:NativeInvertedBoolToVisibilityConverter x:Key="InvertedBoolToVisibilityConverter" />
    <converters:EnumLocalizationConverter x:Key="EnumLocalizationConverter" />
	<converters:ValueMatchVisibilityConverter x:Key="ValueMatchVisibilityConverter" />
</ResourceDictionary>
