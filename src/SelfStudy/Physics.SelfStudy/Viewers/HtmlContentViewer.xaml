<UserControl
    x:Class="Physics.SelfStudy.Viewers.HtmlContentViewer"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:html="using:Physics.SelfStudy.Html"
    xmlns:local="using:Physics.SelfStudy.Viewers"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    VerticalAlignment="Stretch"
    d:DesignHeight="300"
    d:DesignWidth="400"
    mc:Ignorable="d">

    <StackPanel>
        <local:ContentHeader Content="{x:Bind HtmlContent, Mode=OneWay}" />
        <html:MathWebView x:Name="Web" />
    </StackPanel>
</UserControl>
