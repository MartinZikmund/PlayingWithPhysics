<Application
    x:Class="Physics.SelfStudy.Editor.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Physics.SelfStudy.Editor"
    xmlns:resources="using:Physics.SelfStudy.Resources"
    xmlns:resources1="using:Physics.SelfStudy.Editor.Resources"
    RequestedTheme="Light">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <XamlControlsResources xmlns="using:Microsoft.UI.Xaml.Controls" />
                <resources:SelfStudyResources />
                <resources1:EditorResources />
            </ResourceDictionary.MergedDictionaries>
            <Color x:Key="AppThemeColor">#0072b0</Color>
            <Color x:Key="AppTitleBarColor">#0D2B4A</Color>
            <Color x:Key="DarkAppThemeColor">#0072b0</Color>
        </ResourceDictionary>
    </Application.Resources>
</Application>
