    xmlns:editors="using:Physics.SelfStudy.Editor.Editors">

    <DataTemplate x:Key="HtmlContentEditorTemplate">
        <editors:HtmlEditor Grid.Row="1" />
    </DataTemplate>


    <local:EditorDataTemplateSelector x:Key="EditorDataTemplateSelector" HtmlContent="{StaticResource HtmlContentEditorTemplate}" />


</ResourceDictionary>
